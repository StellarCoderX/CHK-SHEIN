{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node_modules/puppeteer-extra/**", "node_modules/puppeteer-extra-plugin-stealth/**", "node_modules/puppeteer-extra-plugin-user-preferences/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
